package com.leetcode;

public class firstPrgm {
    static int[] nums = {3,2,4};
    public static void main(String[] args) {
    	for(int i=0;i<nums.length;i++) {
    		if(i<nums.length-1) {
    		if(nums[i]+nums[i+1]==6) {
    			System.out.print("["+i+","+(i+1)+"]");
    		}
    		}
    	}
    }
}
